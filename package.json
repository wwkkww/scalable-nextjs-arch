{
  "name": "nextjs-fullstack-app-template",
  "author": "Kevin Wong",
  "description": "A tutorial and template for creating a production-ready fullstack Next.js application",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=16.14.2",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
